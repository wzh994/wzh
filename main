//#include<algorithm>
//#include<fstream>
//#include<iostream>
//#include<cstdlib>
//#include<string>
#include"sudoku.h"
#include<time.h>
using namespace std;


//初始化九宫格
Sudoku::Sudoku()
{
	for (int i = 0; i < 11; i++)
	{
		for (int j = 0; j < 11;j++)
		{
			range[i][j] = 0;
		}
	}
}

int main(int argc,char*argv[])
{
	int n = 1000000;
	Sudoku w;
	Judge j;
	char q;
	q = j.judge(argc, argv);
	if (q == 'c')
	{
		int len = strlen(argv[2]);
		int num = 0;
		int flag = 0;
		for (int i = 0; i < len; i++)
		{
			if(argv[2][i] >= '0'&&argv[2][i] <= '9')//如果这个数是数字的话
				num = num * 10 + (argv[2][i] - '0');
			else cout<<"error:请输入数字"<<endl;
		}
		w.createSudoku(num);
	}
	else if (q == 's')
	{
		string path;
		path = argv[2];
		w.solveSudoku(path);
	}
	else if (q == 'e')
	{
		cout<<"error:输入格式错误，sudoku -s/-c 生成个数/结果地址"<<endl;
	}
	else if (q == 'q')
	{
		cout << "error:未定义操作，请输入-c或-s" << endl;
	}
	printf("\n用时：%.2lfs", (double)clock() / CLOCKS_PER_SEC);//调用clock（）函数得出运行时间
	return 0;
}
